{
  "name": "tracecamp-gatsby-demo",
  "private": true,
  "description": "A demo to get up and developing quickly with Gatsby",
  "version": "0.1.0",
  "author": "John L. Armstrong IV",
  "dependencies": {
    "bootstrap": "^4.5.0",
    "cross-env": "^7.0.2",
    "gatsby": "^2.23.3",
    "gatsby-image": "^2.4.7",
    "gatsby-plugin-manifest": "^2.4.11",
    "gatsby-plugin-offline": "^3.2.9",
    "gatsby-plugin-react-helmet": "^3.3.4",
    "gatsby-plugin-sharp": "^2.6.11",
    "gatsby-remark-images": "^3.3.11",
    "gatsby-remark-relative-images": "^0.3.0",
    "gatsby-source-filesystem": "^2.3.11",
    "gatsby-transformer-remark": "^5.25.1",
    "gatsby-transformer-sharp": "^2.5.5",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^6.1.0",
    "reactstrap": "^8.4.1",
    "slugify": "^1.4.0"
  },
  "devDependencies": {
    "prettier": "2.0.5",
    "surge": "^0.21.3"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "cross-env GATSBY_GRAPHQL_IDE=playground gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "presurge": "npm run build",
    "surge": "npx surge --domain $(node -p \"require('./package.json').name\").surge.sh --project ./public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jlarmstrongiv/tracecamp-gatsby-demo"
  },
  "bugs": {
    "url": "https://github.com/jlarmstrongiv/tracecamp-gatsby-demo/issues"
  }
}
